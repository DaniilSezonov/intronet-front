{
  "name": "@insomnia/intro-front",
  "version": "1.0.0",
  "description": "Intronet web site for insomnia fest",
  "scripts": {
    "compile": "cmmn compile --watch",
    "serve": "cmmn serve -b",
    "run": "cmmn spawn compile --watch bundle --watch serve -b",
    "bundle:watch": "cmmn bundle -b --watch",
    "bundle:stats": "cmmn bundle -b --prod --stats",
    "build": "cmmn compile -b && cmmn bundle -b --prod",
    "story": "start-storybook -p 6006",
    "ci": "docker build . -t insomnia/intronet-front",
    "server": "nodemon ./dist/esm/server"
  },
  "type": "module",
  "dependencies": {
    "@cmmn/cell": "1.7.19",
    "@cmmn/tools": "1.7.19",
    "@fastify/websocket": "^7.1.3",
    "@types/luxon": "^2.3.1",
    "@types/node": "17.x.x",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-upsert": "^2.2.6",
    "classnames": "^2.3.2",
    "fastify": "^4.15.0",
    "get-browser-fingerprint": "^3.0.0",
    "log4js": "^6.9.1",
    "log4js-logstash-tcp": "^2.0.5",
    "pouchdb": "^8.0.1",
    "pouchdb-find": "^8.0.1",
    "pouchdb-node": "^8.0.1",
    "pouchdb-upsert": "^2.2.0",
    "preact": "^10.13.0",
    "qr-scanner": "^1.4.1"
  },
  "devDependencies": {
    "@notionhq/client": "^2.2.5",
    "@swc/core": "^1.3.39",
    "@swc/jest": "^0.2.24",
    "@types/classnames": "^2.3.1",
    "husky": ">=6",
    "linkedom": "^0.14.25",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "4.6.x"
  },
  "author": "",
  "license": "ISC",
  "proxy": "https://intro.cherepusick.keenetic.name",
  "cmmn": {
    "main": {
      "input": "dist/esm/src/index.js",
      "module": "es",
      "port": 5001,
      "browser": "true",
      "styles": "modules",
      "inject": "json",
      "html": "src/index.html",
      "mount": {
        "/public": "./public"
      },
      "proxy": {
        "/webapi": "http://localhost:5002"
      }
    },
    "sw": {
      "input": "dist/esm/service-worker/index.js",
      "module": "es"
    },
    "sw-loader": {
      "input": "dist/esm/service-worker/loader.js",
      "module": "es"
    }
  }
}
