{
  "name": "@insomnia/intro-front",
  "version": "1.0.0",
  "description": "Intronet web site for insomnia fest",
  "scripts": {
    "compile": "cmmn compile --watch",
    "serve": "cmmn serve -b",
    "run": "cmmn spawn compile --watch bundle --watch serve -b",
    "bundle:watch": "cmmn bundle -b --watch",
    "bundle:stats": "cmmn bundle -b --prod --stats",
    "build": "cmmn compile -b && cmmn bundle -b --prod",
    "story": "start-storybook -p 6006",
    "ci": "docker build . -t insomnia/intronet-front",
    "server": "nodemon ./dist/esm/server"
  },
  "type": "module",
  "dependencies": {
    "@chakra-ui/icons": "^2.0.0",
    "@chakra-ui/react": "^2.0.2",
    "@cmmn/cell": "1.7.12",
    "@cmmn/tools": "1.7.12",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fastify/websocket": "^7.1.3",
    "@types/luxon": "^2.3.1",
    "@types/node": "17.x.x",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-upsert": "^2.2.6",
    "pouchdb": "^8.0.1",
    "pouchdb-find": "^8.0.1",
    "pouchdb-node": "^8.0.1",
    "pouchdb-upsert": "^2.2.0",
    "fastify": "^4.15.0",
    "chakra-react-select": "^4.0.0",
    "chakra-ui-markdown-renderer": "^4.1.0",
    "cookie": "^0.5.0",
    "formik": "^2.2.9",
    "framer-motion": "^6",
    "indexeddb-wrapper": "^0.1.2",
    "luxon": "2.x.x",
    "preact": "^10.13.0",
    "qr-scanner": "^1.4.1",
    "react-markdown": "^8.0.3",
    "@storybook/addon-actions": "6.5.0-rc.0",
    "@storybook/addon-essentials": "6.5.0-rc.0",
    "@storybook/addon-links": "6.5.0-rc.0",
    "@storybook/builder-webpack5": "6.5.0-rc.0",
    "@storybook/manager-webpack5": "6.5.0-rc.0",
    "@storybook/preset-create-react-app": "4.x.x",
    "@storybook/react": "6.5.0-rc.0",
    "@swc/core": "^1.3.39",
    "@swc/jest": "^0.2.24",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^28.0.2"
  },
  "author": "",
  "license": "ISC",
  "proxy": "https://intro.cherepusick.keenetic.name",
  "cmmn": {
    "main": {
      "input": "dist/esm/src/index.js",
      "module": "es",
      "port": 5001,
      "browser": "true",
      "styles": "modules",
      "inject": "json",
      "html": "src/index.html",
      "mount": {
        "/public": "./public"
      },
      "proxy": {
        "/api": "http://localhost:5002"
      }
    },
    "sw": {
      "input": "dist/esm/service-worker/index.js",
      "module": "es"
    },
    "sw-loader": {
      "input": "dist/esm/service-worker/loader.js",
      "module": "es"
    }
  }
}
